<div>
  <div class="creative-container">
    <% @creatives_box1.each do |creative| %>
          <div class="<%= creative.dimensions %>">
            <%= link_to(image_tag(creative.image.url), creative.link) %>
          </div>
    <% end %>
  </div>
  <div class="creative-container">
    <% @creatives_tall.each do |creative| %>
        <div class="<%= creative.dimensions %>">
          <%= link_to(image_tag(creative.image.url), creative.link) %>
        </div>
    <% end %>
  </div>
  <br style="clear:both">
  <div class="creative-container">
	<% @creatives_wide.each do |creative| %>
		<div class="<%= creative.dimensions %>">
		  <%= link_to(image_tag(creative.image.url), creative.link) %>
		</div>
	<% end %>
  </div>
  <div class="creative-container">
	<% @creatives_box2.each do |creative| %>
		<div class="<%= creative.dimensions %>">
		  <%= link_to(image_tag(creative.image.url), creative.link) %>
		</div>
	<% end %>
  </div>
</div>
